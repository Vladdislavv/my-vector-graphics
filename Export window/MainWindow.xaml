<Window x:Class="Export_window.ExportDiologWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Export_window"
        mc:Ignorable="d"
        Title="Export" Height="200" Width="400" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="export" Content="Export" HorizontalAlignment="Left" Margin="0,107,0,0" VerticalAlignment="Top" Height="27" Width="112" Click="export_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="288,107,0,0" VerticalAlignment="Top" Height="27" Width="112" Click="Cancel_Click"/>
        <TextBox x:Name="path" HorizontalAlignment="Left" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="373" Height="20" Margin="0,31,0,0"/>
        <Label Content="name of file" HorizontalAlignment="Left" Margin="0,51,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="name_of_file" HorizontalAlignment="Left" Margin="0,77,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="export path" HorizontalAlignment="Left" Margin="-2,10,0,0" VerticalAlignment="Top"/>
    </Grid>

</Window>
